<Window x:Class="LVRS_Database.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LVRS_Database"
        mc:Ignorable="d"
        Title="LVRS Database Home" Height="600" Width="1200">
    <DockPanel>
        <TabControl DockPanel.Dock="Top" >
            <TabItem Header="Home" IsSelected="True">
                <Frame Source="Home.xaml"/>
            </TabItem>
            <TabItem Header="Add Member">
                <Frame Source="AddMember.xaml" />
            </TabItem>
            <TabItem Header="Add Member Details">
                <Frame Source="AddMemberDetails.xaml" />
            </TabItem>
            <TabItem Header="View Member Details"/>
        </TabControl>
    </DockPanel>
</Window>
